{
  "cli": {
    "version": ">= 15.0.12",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "enterpriseProvisioning": "adhoc",
        "credentialsSource": "remote"
      }
    },
    "preview": {
      "distribution": "store",
      "ios": {
        "credentialsSource": "remote",
        "buildConfiguration": "Release"
      },
      "env": {
        "ENVIRONMENT": "production"
      }
    },
    "production": {
      "autoIncrement": true,
      "distribution": "store",
      "ios": {
        "credentialsSource": "remote",
        "buildConfiguration": "Release"
      },
      "env": {
        "ENVIRONMENT": "production"
      }
    }
  },
  "submit": {
    "production": {
      "ios": {
